<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.dingcan.dao.UserDao">
	<select id="doFindIdByUserName" resultType="int">
		select id from t_user 
		where username=#{name}
	</select>
	<select id="doFindObjectById" resultType="string">
		select username from t_user
		where id=#{id}
	</select>
</mapper>